{
  "private": true,
  "name": "@adaliszk/node-toolbox",
  "description": "AdaLiszk's frequently used NPM packages wrapped up into easy-to-use packs",
  "repository": "github:adaliszk/node-toolbox",
  "homepage": "https://github.com/adaliszk/node-toolbox",
  "bugs": "https://github.com/adaliszk/node-toolbox/issues",
  "license": "BSD-2-Clause",
  "author": "Ádám Liszkai <node-toolbox@adaliszk.dev>",
  "contributors": [],
  "scripts": {
    "build": "turbo run build --output-logs=new-only",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "fmt": "dprint fmt",
    "propose": "turbo run build lint test && changeset",
    "release": "turbo run build && changeset version && changeset publish"
  },
  "packageManager": "pnpm@8.2.0",
  "devDependencies": {
    "@adaliszk/typescript": "^5.0.1",
    "@changesets/changelog-git": "^0.1.14",
    "@changesets/cli": "^2.26.1",
    "@swc/core": "^1.3.53",
    "dprint": "^0.45.0",
    "esbuild": "^0.17",
    "pkg": "^5.8.1",
    "tsup": "^6.7",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4"
  },
  "eslintConfig": { "extends": ["@adaliszk/typescript"] },
  "engines": { "node": ">=18", "pnpm": ">=8.3" },
  "keywords": ["node", "npm", "toolbox", "packages", "configs", "plugins", "packs"]
}
