{
  "private": true,
  "name": "@adaliszk/node-toolbox",
  "description": "AdaLiszk's frequently used NPM packages wrapped up into easy-to-use packs",
  "repository": "github:adaliszk/node-toolbox",
  "homepage": "https://github.com/adaliszk/node-toolbox",
  "bugs": "https://github.com/adaliszk/node-toolbox/issues",
  "license": "BSD-3-Clause",
  "author": "Ádám Liszkai <node-toolbox@adaliszk.dev>",
  "contributors": [],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "release": "turbo run build lint test && changeset version && changeset publish",
    "changeset": "changeset",
    "turbo": "turbo",
    "workspace": "yarn workspace"
  },
  "workspaces": [
    "configs/*",
    "packages/*",
    "specifications/*",
    "stacks/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.26",
    "turbo": "^1.6"
  },
  "keywords": [
    "node",
    "npm",
    "toolbox",
    "packages",
    "configs",
    "plugins",
    "packs"
  ],
  "packageManager": "yarn@3.3.1"
}
