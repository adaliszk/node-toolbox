{
  "$schema" : "https://turborepo.org/schema.json",
  "globalDependencies": [
    "typescript",
    "eslint",
    "tsup",
    "@swc/core"
  ],
  "pipeline" : {
    "build" : {
      "dependsOn" : [
        "^build"
      ],
      "outputs" : [
        "dist/**",
        "server/**"
      ]
    },
    "test": {
      "dependsOn" : ["build"]
    },
    "lint": {
      "dependsOn" : []
    }
  }
}
