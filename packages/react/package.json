    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@adaliszk/typescript": "^4.9.5",
    "@swc/core": "latest",
    "eslint": "latest",
    "tsup": "latest",
    "typescript": "latest"
  },
  "peerDependencies": {
    "eslint": "^8.33",
    "typescript": "^4.9"
  },
  "keywords": [
    "typescript",
    "fission",
    "functions",
    "types"
