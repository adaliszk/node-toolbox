    "vitest": "^1.1.0"
  },
  "devDependencies": {
    "@adaliszk/typescript": "workspace:~",
    "@swc/core": "^1.3.101",
    "eslint": "^8",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "typescript",
    "testing",
    "jest",
    "TDD",
    "BDD"
