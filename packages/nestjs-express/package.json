  "devDependencies": {
    "@adaliszk/typescript": "workspace:*",
    "@swc/core": "^1.3",
    "esbuild": "^0.19",
    "eslint": "^8.56",
    "tsup": "^6.7",
    "typescript": "^5.3"
  },
  "keywords": [
    "typescript",
    "nest.js",
    "api"
