{
  "version": "1.0.1",
  "name": "@adaliszk/qwik",
  "description": "Qwik stack and configuration for Development using Typescript",
  "repository": "github:adaliszk/node-toolbox",
  "homepage": "https://github.com/adaliszk/node-toolbox/tree/release/packages/qwik",
  "bugs": "https://github.com/adaliszk/node-toolbox/labels/qwik",
  "license": "BSD-3-Clause",
  "types": "dist/bundle.d.ts",
  "module": "dist/bundle.mjs",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "tsup source/bundle.ts --format cjs,esm --minify --sourcemap --dts",
    "lint": "eslint source/**",
    "prepublish": "pnpm build"
  },
  "eslintConfig": {
    "extends": [
      "@adaliszk/qwik"
    ]
  },
  "dependencies": {
    "@adaliszk/eslint-config-qwik": "^1.0.1",
    "@builder.io/qwik": "^1.0.0",
    "@builder.io/qwik-city": "^1.0.0",
    "@types/node": "^18",
    "undici": "^5.22.0"
  },
  "devDependencies": {
    "@adaliszk/typescript": "^5.0.1",
    "@swc/core": "^1.3.56",
    "eslint": "^8.39.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "eslint": "^8.39",
    "typescript": "^5.0",
    "undici": "^5.22"
  },
  "keywords": [
    "typescript",
    "qwik",
    "resumable react",
    "static-content generator",
    "server-side renderer",
    "react-like"
  ],
  "exports": {
    ".": {
      "require": "./tsconfig.json",
      "import": "./dist/bundle.mjs"
    },
    "./config": {
      "require": "./tsconfig.json",
      "import": "./tsconfig.json"
    }
  },
  "files": [
    "dist",
    "*.json",
    "*.md"
  ]
}
