{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": true,
    "declaration": true,
    "sourceMap": true,
    "strict": true
  },
  "exclude": [
    "**/node_modules"
  ]
}